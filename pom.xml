<!-- Maven plug in to read a property file for cc.properties and BuildFaces.properties-->
		<plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>properties-maven-plugin</artifactId>
		<version>1.0-alpha-2</version>
        <executions>
           <!-- Associate the read-project-properties goal with the initialize phase, to read the properties file. -->
          <execution>
            <phase>initialize</phase>
            <goals>
              <goal>read-project-properties</goal>
            </goals>
            <configuration>
              <files>
                <file>${basedir}/cc.properties</file>
				<!--<file>E:/CIT_Config/cruisecontrol-bin-2.8.4/BuildFacesConfig.properties</file>-->
				<file>D:/Internaltools/BuildFaces/CIT_Config/cruisecontrol-bin-2.8.4/BuildFacesConfig.properties</file>
              </files>
            </configuration>
          </execution>
        </executions>
      </plugin>

	  




